                        android:foreground="?attr/selectableItemBackground"
                        android:paddingTop="@dimen/material_component_cards_left_and_right_padding"
                        android:paddingBottom="@dimen/material_component_cards_left_and_right_padding">

                        <TextView
                            android:id="@+id/tv_quit_smoke_date_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
